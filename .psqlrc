\set QUIET 1

\pset null [null]
\pset pager off
\set COMP_KEYWORD_CASE upper
\set HISTCONTROL ignoredups
\set HISTFILE ~/.psql/history- :DBNAME
\set ON_ERROR_ROLLBACK interactive
#\set PROMPT1 '%[%033[1m%]%M/%/%R%[%033[0m%]%# '
\set PROMPT1 '%[%033[1m%][%/] # '
\set PROMPT2 '... > '
\set VERBOSITY verbose
\timing
\x auto

\unset QUIET
